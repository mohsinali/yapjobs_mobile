<input id="current_position" name="current_position" type="hidden" /> 

<div class="yp-header">
<div class="yp-header-left-btn">
        <a href="<%=root_path %>"><div class="yp-header-left-btn-icon"><%= image_tag "yp-list-header-back-icon.png", width: "13", height: "22" %></div></a>
  </div>
    <div class="yp-header-logo">
        <div class="yp-header-logo-icon"><%= image_tag "yp-home-yapjobs-logo.png" %></div>
    </div>
    <div class="yp-header-next-btn">
     <a href="set.html"><div class="yp-header-next-btn-icon"><%= image_tag "yp-list-header-setting-icon.png", width: "23", height: "23" %></div></a>
    </div>
    <div class="clearBoth"></div>
</div>

<%- @jobs["jobs"].each do |job| %>
    <a href="jobs/<%= job["id"] %>">
    <div class="yp-list">
        <div class="yp-list-left">
            <div class="yp-list-left-icon">        
            <%= image_tag "http://www.yapjobs.com/" + job["role"]["image"], width: "26", height: "27" %>

            </div>
      </div>
        <div class="yp-list-content">
            <div class="yp-list-content-job-title"><%= job["role"]["name"] %></div>
                <div class="yp-list-content-address"><%= job["business"]["address"] %></div>
                    <div class="yp-list-content-footer">
                    <div class="yp-list-content-footer-distance"><%= job["distance"] %> miles away</div>
                    <div class="yp-list-content-footer-salary"><%= job["hourly_rate"] %> ph</div>
                    <div class="clearBoth"></div>
                </div>
        </div>
        <div class="clearBoth"></div>

    </div>
    </a>
<% end %>
